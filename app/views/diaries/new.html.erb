<div class="wrap">
<%= render 'header' %>
<%= render 'nav' %>
<div class="container">
<%= simple_form_for @diary do |form| %>


  <div><%= form.input :content, label: false, placeholder: '輸入內容' %></div>
  
  <div><%= form.input :diary_date, as: :date, html5: true,order: [:year,:month,:day],label: false %></div>

  <div>選擇地點</div>

  <div>標籤</div>
  <div><%= link_to '新增旅遊圖片', new_picture_path %></div>

  <%= form.submit '送出'%>


<% end %>
</div>
</div>