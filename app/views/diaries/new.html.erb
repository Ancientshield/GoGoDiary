<div class="wrap">
<%= simple_form_for @diary do |form| %>


  <div><%= form.input :content, label: false, placeholder: '輸入內容' %></div>
  
  <div><%= form.input :diary_date, as: :date, html5: true,label: false %></div>

  <div>選擇地點</div>

  <div>標籤</div>
  <div>圖片<%= form.submit '新增照片'%></div>

  <%= form.submit '送出'%>
  


<% end %>
</div>